{
    "name": "ilbronza/fileimporter",
    "description": ":import files",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Bronzini",
            "email": "davide@sistema.srl",
            "homepage": "https://sistema.srl"
        }
    ],
    "homepage": "https://github.com/ilbronza/fileimporter",
    "keywords": ["Laravel", "FileImporter"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "IlBronza\\FileImporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IlBronza\\FileImporter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IlBronza\\FileImporter\\FileImporterServiceProvider"
            ],
            "aliases": {
                "FileImporter": "IlBronza\\FileImporter\\Facades\\FileImporter"
            }
        }
    }
}
